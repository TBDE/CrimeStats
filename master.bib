@InProceedings{ farkas2005dra,
	title = {{Distributed Resilient Architecture for Ethernet Networks}},
	author = "J{\'a}nos Farkas and Csaba Antal and G{\'a}bor T{\'o}th and Lats Westberg",
	booktitle = "Proc. 5th Int. Workshop on Design of Reliable Communication Networks",
	pages = 8,
	year = 2005,
	abstract = "We propose an enhanced resilient Ethernet architecture that is based on multiple pre-established spanning trees to connect a set of edge nodes. Fast failover is achieved so that edge nodes switch to a non-affected tree after a link or node failure. We propose a fast and robust procedure for edge nodes to detect failures and notify other edge nodes. The other contribution of the paper is an algorithm that calculates multiple spanning trees for input topologies so that at least one tree provides connectivity after a node or a link failure. The algorithm provides minimal number of spanning trees to make input networks fault tolerant."
}

